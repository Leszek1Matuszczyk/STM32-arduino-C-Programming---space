.PHONY: clean All

All:
	@echo "----------Building project:[ switch_instruction - Debug ]----------"
	@cd "switch_instruction" && "$(MAKE)" -f  "switch_instruction.mk"
clean:
	@echo "----------Cleaning project:[ switch_instruction - Debug ]----------"
	@cd "switch_instruction" && "$(MAKE)" -f  "switch_instruction.mk" clean
