.PHONY: clean All

All:
	@echo "----------Building project:[ for_loop_continuation - Debug ]----------"
	@cd "for_loop_continuation" && "$(MAKE)" -f  "for_loop_continuation.mk"
clean:
	@echo "----------Cleaning project:[ for_loop_continuation - Debug ]----------"
	@cd "for_loop_continuation" && "$(MAKE)" -f  "for_loop_continuation.mk" clean
